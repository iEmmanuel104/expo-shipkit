{
  "name": "expo-shipkit",
  "version": "2.0.0",
  "description": "Streamlined deployment workflow for Expo/EAS applications",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "shipkit": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "local:link": "npm run build && npm link"
  },
  "keywords": [
    "expo",
    "eas",
    "deployment",
    "cli",
    "react-native",
    "ios",
    "android",
    "app-store",
    "google-play",
    "mobile",
    "release",
    "shipping"
  ],
  "author": "Emmanuel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iemmanuel104/expo-shipkit"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "eas-cli": ">=10.0.0"
  },
  "peerDependenciesMeta": {
    "eas-cli": {
      "optional": true
    }
  },
  "dependencies": {
    "commander": "^12.1.0",
    "@inquirer/prompts": "^7.2.1",
    "zod": "^3.24.1",
    "picocolors": "^1.1.1",
    "ora": "^8.1.1",
    "cosmiconfig": "^9.0.0",
    "cosmiconfig-typescript-loader": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "memfs": "^4.15.0"
  }
}
